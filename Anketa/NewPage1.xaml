<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Anketa.NewPage1">
    <StackLayout Padding="20">
        <Image x:Name="UserPhoto" HeightRequest="200" WidthRequest="200" />
        <Label x:Name="FullNameLabel" FontSize="24" />
        <Label x:Name="GenderLabel" FontSize="20" />
        <Label x:Name="AgeLabel" FontSize="20" />
        <Label x:Name="HostelLabel" FontSize="20" />
        <Label x:Name="LeaderLabel" FontSize="20" />

        <Button Text="Назад" Clicked="BackButton_Clicked" />
    </StackLayout>
</ContentPage>